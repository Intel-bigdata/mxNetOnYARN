#!/bin/bash
# Kvstore log for Debug
#export PS_VERBOSE=1
#ps-lite/tracker/dmlc_local.py -n 2 -s 2 \


#echo $MODEL
NUM_WORKER=2
MXNET_HOME="../mxnet"
LAUNCH_SCRIPT="${MXNET_HOME}/tools/launch.py"
JOB=""

if [[ -n $1 ]]; then
  MODEL="$1"
else
  MODEL="local"
fi

if [[ -n $2 ]]; then
  JOB="$2"
else
  echo "Please specify a python job, e.g., job.py"
  exit
fi

if [[ -n $3 ]]; then
  DATA_DIR="$3"
else
  DATA_DIR="data/"
fi

$LAUNCH_SCRIPT -n $NUM_WORKER --launcher $MODEL python $JOB --data-dir=$DATA_DIR $@
